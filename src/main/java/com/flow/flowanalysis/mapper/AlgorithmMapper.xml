<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.flow.flowanalysis.mapper.AlgorithmMapper">
    <resultMap type="com.flow.flowanalysis.model.Algorithm" id="BaseInfo">
        <id column="id" property="id"/>
        <result property="name" column="name" />
        <result property="zhName" column="zhname" />
        <result property="description" column="description" />
    </resultMap>
    <select id="getAllalgor" resultMap="BaseInfo">
          select * from algorithm
    </select>
</mapper>